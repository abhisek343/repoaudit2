# Archive download settings
USE_ARCHIVE_DOWNLOAD=true
ARCHIVE_TIMEOUT_MS=60000
MAX_ARCHIVE_SIZE=100MB

# Rate limiting (for fallback)
RATE_LIMIT_DELAY_MS=1000
MAX_CONTENT=800

# GitHub API settings
GITHUB_TOKEN=your_github_token_here

# LLM settings (for AI analysis)
LLM_PROVIDER=openai
LLM_API_KEY=your_llm_api_key_here
LLM_MODEL=gpt-4

# Cache settings
REDIS_URL=redis://localhost:6379
CACHE_TTL=3600
